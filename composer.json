	"type": "yii2-extension",

	"description": "Yii2-hyperlinks is a module and set of functionality to add hyperlinks to a model in a generic way",
	"keywords": ["yii2", "hyperlink", "hyperlinks", "module", "behavior", "widget", "bundle", "action"],

	"license": "MIT",
	"authors": [
		{
			"name": "Pascal Mueller",
			"email": "pascal.mueller@asinfotrack.ch",
			"homepage": "http://www.asinfotrack.ch"
		}
	],

	"minimum-stability": "stable",
	"support": {
		"source": "https://github.com/asinfotrack/yii2-hyperlinks",
		"issues": "https://github.com/asinfotrack/Yii2-hyperlinks/issues"
	},

	"require": {
		"php": ">=5.4.0",

		"yiisoft/yii2": "~2.0.6",
		"yiisoft/yii2-bootstrap": "~2.0.0",

		"asinfotrack/yii2-toolbox": "~0.8.5"
	},

	"autoload": {
		"psr-4": {
			"asinfotrack\\yii2\\hyperlinks\\": ""
		}
